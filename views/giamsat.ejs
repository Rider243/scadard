    </head>
<body onload="
chart_realtime('chart_line_voltage','L1_line','L2_line','L3_line','Line L1-L2','Line L2-L3','Line L3-L1');
chart_realtime('chart_phase_voltage','L1_phase','L2_phase','L3_phase','L1 Phase','L2 Phase','L3 Phase');
chart_realtime('chart_current','L1_phase_cr','L2_phase_cr','L3_phase_cr','L1 Current','L2 Current','L3 Current');

">
    <%- include('header.ejs'); -%>
    <div class="main">
        <div class="container_gauges">     
            <div class="row1">
                <div class="gauges"    >
                    <div id="gauge_L1_lines"></div>
                    <p class="infogauge">Line L1-L2</p>
                </div>
                
                <script>gauge_temp_1('gauge_L1_lines','L1_line');</script>
    
    
                <div class="gauges"    >
                    <div id="gauge_L2_lines"></div>
                    <p class="infogauge">Line L2-L3</p>
                </div>
                <script>gauge_temp_1('gauge_L2_lines','L2_line');</script>
    
                <div class="gauges"    >
                    <div id="gauge_L3_lines"></div>
                    <p class="infogauge">Line L3-L1</p>
                </div>
                <script>gauge_temp_1('gauge_L3_lines','L3_line');</script>
            </div>
            <div class="row1">
                <div class="gauges"     >
                    <div id="gauge_L1_phases"></div>
                    <p class="infogauge">Phase L1</p>
                </div>
                <script>gauge_temp_2('gauge_L1_phases','L1_phase');</script>
                <!-- <p>
                    Điện áp tối đa hôm nay là
                    <p id="infor1"></p>
                    <script>fn_IOFieldDataShow('L1_phase', 'infor1', 1)</script>
                </p> -->
             
                    
                <div class="gauges"    >
                    <div id="gauge_L2_phases"></div>
                    <p class="infogauge">Phase L2</p>
                </div>
                <script>gauge_temp_2('gauge_L2_phases','L2_phase');</script>
    
                    
                <div class="gauges"   >
                    <div id="gauge_L3_phases"></div>
                    <p class="infogauge">Phase L3</p>
                </div>
                <script>gauge_temp_2('gauge_L3_phases','L3_phase');</script>
            </div>
            <div class="row1">

                <div class="gauges"     >
                    <div id="gauge_L1_phases_cr"></div>
                    <p class="infogauge">Current L1</p>
                </div>
                <script>gauge_temp_3('gauge_L1_phases_cr','L1_phase_cr','A');</script>

             
                <div class="gauges"    >
                    <div id="gauge_L2_phases_cr"></div>
                    <p class="infogauge">Current L2</p>
                </div>
                <script>gauge_temp_3('gauge_L2_phases_cr','L2_phase_cr');</script>
    
                    
                <div class="gauges"   >
                    <div id="gauge_L3_phases_cr"></div>
                    <p class="infogauge">Current L3</p>
                </div>
                <script>gauge_temp_3('gauge_L3_phases_cr','L3_phase_cr');</script>
            </div>


         </div>

         <div class="container_chart-gauges">

            <div id="chart_line_voltage" class="content_chat"></div>

            <div id="chart_phase_voltage" class="content_chat"></div>

            <div id="chart_current" class="content_chat"></div>
            
         </div>
    </div>
</body>
</html>